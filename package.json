{
  "name": "atomic-fishbowl-client",
  "version": "1.0.2",
  "license": "SEE LICENSE IN atomic-fishbowl.license",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload --public-host https://localhost",
    "startprod": "ng serve --prod --aot --live-reload --live-reload-client https://localhost",
    "stats": "npx webpack-bundle-analyzer dist/stats.json",
    "build": "cp -f node_modules/pdfjs-dist/build/pdf.worker.min.js src/resources/ && cp -f node_modules/isotope-layout/dist/isotope.pkgd.min.js src/resources/ && npm run zipnwcontext && ng build",
    "buildprod": "cp -f node_modules/pdfjs-dist/build/pdf.worker.min.js src/resources/ && cp -f node_modules/isotope-layout/dist/isotope.pkgd.min.js src/resources/ && npm run zipnwcontext && ng build --prod",
    "copypdf": "cp -f node_modules/pdfjs-dist/build/pdf.worker.min.js src/resources",
    "zipnwcontext": "cd src/nw-context-menu && zip ../resources/nw-investigation-context-menu.zip nw-investigation-*.json 'AFB-NW Context Menu Integration Guide.pdf'",
    "bumppatch": "npm --no-git-tag-version version patch && npm run writeBuildProperties",
    "writeBuildProperties": "./writeBuildProperties.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.10",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^10.0.10",
    "@angular/compiler": "^10.0.10",
    "@angular/core": "^10.0.10",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^10.0.10",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^10.0.10",
    "@angular/platform-browser-dynamic": "^10.0.10",
    "@angular/router": "^10.0.10",
    "@juggle/resize-observer": "^3.1.0",
    "@kensingtontech/dragula": "^3.7.3",
    "angular2-uuid": "^1.1.1",
    "animate-css-grid": "^1.4.3",
    "dayjs": "^1.8.33",
    "font-awesome": "^4.7.0",
    "imagesloaded": "^4.1.3",
    "isotope-layout": "^3.0.6",
    "jsencrypt": "^3.0.0-rc.1",
    "loglevel": "^1.6.7",
    "ng-animate": "^0.3.4",
    "ng2-dragula": "^2.1.1",
    "ng2-panzoom": "^10.0.0",
    "ng2-pdf-viewer": "^5.3.4",
    "primeicons": "^2.0.0",
    "primeng": "^9.0.0",
    "rxjs": "~6.5.4",
    "screenfull": "^5.0.2",
    "socket.io-client": "^2.3.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "^10.0.10",
    "@angular/language-service": "^10.0.10",
    "@types/imagesloaded": "^4.1.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/mathjs": "0.0.35",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^1.4.32",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
