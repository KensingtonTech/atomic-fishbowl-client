{
  "name": "atomic-fishbowl-client",
  "version": "1.0.4",
  "license": "GPL-3.0",
  "scripts": {
    "postinstall": "scripts/postinstall.sh",
    "ng": "ng",
    "start": "ng serve",
    "startprod": "ng serve --configuration production --aot --live-reload --live-reload-client https://localhost",
    "stats": "npm run build:prod:stats && webpack-bundle-analyzer dist/stats.json",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "build:prod": "ng build --configuration production",
    "build:prod:stats": "ng build --configuration production --stats-json",
    "build:gzip": "npm run build && cd dist && gzip -r *",
    "build:prod:gzip": "npm run build:prod && cd dist && gzip -r *",
    "serve-build": "cd dist && npx http-server -p 3000",
    "serve-build:gzip": "cd dist && npx http-server -g -p 3000",
    "zipnwcontext": "cd src/nw-context-menu && zip ../resources/nw-investigation-context-menu.zip nw-investigation-*.json 'AFB-NW Context Menu Integration Guide.pdf'",
    "bumppatch": "npm --no-git-tag-version version patch && npm run writeBuildProperties",
    "writeBuildProperties": "scripts/writeBuildProperties.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.0.0",
    "@angular/compiler": "^13.0.0",
    "@angular/core": "^13.3.9",
    "@angular/forms": "^13.0.0",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "^13.0.0",
    "@angular/platform-browser-dynamic": "^13.0.0",
    "@angular/router": "^13.0.0",
    "@kensingtontech/dragula": "^3.7.3",
    "dayjs": "^1.11.2",
    "font-awesome": "^4.7.0",
    "imagesloaded": "^5.0.0",
    "isotope-layout": "^3.0.6",
    "jsencrypt": "^3.2.1",
    "loglevel": "^1.6.7",
    "ng-animate": "^0.3.4",
    "ng2-dragula": "^2.1.1",
    "ng2-pdf-viewer": "^9.0.0",
    "ngx-panzoom": "^13.0.0",
    "primeflex": "^3.2.1",
    "primeicons": "^5.0.0",
    "primeng": "^13.4.0",
    "rxjs": "~7.5.5",
    "screenfull": "^6.0.1",
    "socket.io-client": "^4.5.1",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.6",
    "@angular-eslint/builder": "^13.2.1",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "^13.0.0",
    "@angular/compiler-cli": "^13.0.0",
    "@angular/language-service": "^13.0.0",
    "@types/dragula": "^3.7.1",
    "@types/imagesloaded": "^4.1.2",
    "@types/jasmine": "~3.10",
    "@types/node": "^16.11.38",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "codelyzer": "^6.0.0",
    "eslint": "^8.16.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
