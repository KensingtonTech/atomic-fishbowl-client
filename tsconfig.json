{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "dragula": [
        "node_modules/@kensingtontech/dragula/dist/dragula.min.js"
      ],
      "socket.io-client": [
        "node_modules/socket.io-client/dist/socket.io.slim.js"
      ],
      "types/*": [
        "src/app/types/*"
      ],
      "services/*": [
        "src/app/services/*"
      ],
      "jsencrypt": [
        "node_modules/jsencrypt/bin/jsencrypt.min.js"
      ]
    },
    "allowSyntheticDefaultImports": true
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": false,
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}