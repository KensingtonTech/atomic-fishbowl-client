{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "dragula": [
        "node_modules/dragula/dist/dragula.min.js"
      ],
      "socket.io-client": [
        "node_modules/socket.io-client/dist/socket.io.slim.js"
      ],
      "ng2-panzoom": [
        "dist/ng2-panzoom"
      ],
      "ng2-mousewheel": [
        "dist/ng2-mousewheel"
      ]
    }
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": false
  }
}